class Node
{
    int data;
    Node next;
    Node()
    {

    }
    Node(int data)
    {
        this.data=data;
    }
}
class Linkedlist{
    Node head;
    public Node insert(int data)
    {
        Node node = new Node();
        node.data = data;
        node.next = null;
        if(head==null)
        {
            head = node;
        }
        else
        {
            Node n = head;
            while(n.next!=null)
            {
                n = n.next;
            }
            n.next=node;
        }
        return head;
    }
  }
  class mylinkedlist
{
    public static void main(String[] args) {
        Node x = null;
        Linkedlist list1 = new Linkedlist();
        x=list1.insert(1);
        x=list1.insert(2);
        x=list1.insert(3);
        x=list1.insert(4);
        x = list1.swap(x);
        list1.show(x);
    }
}
